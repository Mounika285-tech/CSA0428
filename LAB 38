#include <stdio.h>
#include <stdlib.h>

void scan(int arr[], int size, int head, int direction) {
    int seek_sequence[size + 1];
    int index = 0;

    // Sort the request array
    for (int i = 0; i < size; i++) {
        for (int j = i + 1; j < size; j++) {
            if (arr[i] > arr[j]) {
                int temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }

    // Find the position of the head
    int pos;
    for (pos = 0; pos < size; pos++) {
        if (arr[pos] >= head) {
            break;
        }
    }

    // Move towards the end
    for (int i = pos; i < size; i++) {
        seek_sequence[index++] = arr[i];
    }
    // Move towards the beginning
    for (int i = pos - 1; i >= 0; i--) {
        seek_sequence[index++] = arr[i];
    }

    printf("Seek Sequence is: ");
    for (int i = 0; i < index; i++) {
        printf("%d ", seek_sequence[i]);
    }
    printf("\n");
}

int main() {
    int requests[] = {176, 79, 34, 60, 92, 11, 41, 114};
    int head = 50;
    int direction = 1; // 1 for up, 0 for down
    int size = sizeof(requests) / sizeof(requests[0]);

    scan(requests, size, head, direction);
    return 0;
}
